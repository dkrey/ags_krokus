
// room script file
function room_Load()
{
  gDebugging.Visible=false;
  gLoading.Visible=false;
  gEditorScene.Visible=false;
  int i = 11;
  while (i < 32) {
    gui[i].Visible=false;
    i++;
  }
  TENG.stop();
  TENG.clean_up();
  TENG.init_game();
  EGO.init_player(); 
  TENG.set_topdown_mode(false);
  player.x = -100;
  player.y = -100;
  leveledit = "";
}

function room_AfterFadeIn()
{

  gSelectStage.Visible=true;
  mouse.Visible = true;
  mouse.Mode = eModePointer; 
}


